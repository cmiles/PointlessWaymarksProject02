<Window x:Class="TheLemmonWorkshopWpfControls.PhotoContentEditor.PhotoContentEditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:TheLemmonWorkshopWpfControls.PhotoContentEditor"
        mc:Ignorable="d"
        Title="PhotoContentEditorWindow" Height="450" Width="800" d:DataContext="{d:DesignInstance local:PhotoContentEditorWindow}">
    <Grid>
        <local:PhotoContentEditorControl DataContext="{Binding PhotoContentEditor}"></local:PhotoContentEditorControl>
    </Grid>
</Window>
