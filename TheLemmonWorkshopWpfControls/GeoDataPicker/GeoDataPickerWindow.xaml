<Window
    x:Class="TheLemmonWorkshopWpfControls.GeoDataPicker.GeoDataPickerWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TheLemmonWorkshopWpfControls.GeoDataPicker"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="GeoDataPickerControlWindow"
    Width="800"
    Height="450"
    d:DataContext="{d:DesignInstance local:GeoDataPickerWindow}"
    mc:Ignorable="d">
    <Grid>
        <local:GeoDataPickerControl DataContext="{Binding GeoDataPickerContext}" />
    </Grid>
</Window>