<Window
    x:Class="PointlessWaymarksCmsWpfControls.GeoDataPicker.GeoDataPickerWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:geoDataPicker="clr-namespace:PointlessWaymarksCmsWpfControls.GeoDataPicker"
    Title="GeoDataPickerControlWindow"
    Width="800"
    Height="450"
    d:DataContext="{d:DesignInstance geoDataPicker:GeoDataPickerWindow}"
    mc:Ignorable="d">
    <Grid>
        <geoDataPicker:GeoDataPickerControl DataContext="{Binding GeoDataPickerContext}" />
    </Grid>
</Window>