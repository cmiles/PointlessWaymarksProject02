<UserControl
    x:Class="PointlessWaymarksCmsWpfControls.WebViewTwoScratch.WebViewTwoControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:PointlessWaymarksCmsWpfControls.WebViewTwoScratch"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
    d:DataContext="{d:DesignInstance local:WebViewTwoContext}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    DataContextChanged="WebViewTwoControl_OnDataContextChanged"
    Loaded="WebViewTwoControl_OnLoaded"
    mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <wpf:WebView2 Name="ControlWebView" CoreWebView2Ready="WebView2_OnCoreWebView2Ready" />
        <Button Grid.Row="1" Command="{Binding RotateHtmlCommand}">Change</Button>
    </Grid>
</UserControl>
